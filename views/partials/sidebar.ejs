<div id="sidebar-wrapper">
            <ul class="sidebar-nav">
                <li class="sidebar-brand">
                </li>
                <li>
                    <a href="/dashboard">Charts</a>
                </li>
                <li>
                    <a href = "/receipts">Receipts</a>
                </li>

                <li><span>Folders</span>
                    <ul>

                        <% for(var i = 0; i < session.folders.length; i ++){ %>
                                <li class = "folder" id = "<%= session.folders[i] %>" style="padding-right: 10px;">
                                    <span class="ico-pull pull-right">
                                     
                                        <img src="/images/glyphicons-31-pencil.png" onclick="renameForm(<%= i %>, '<%=session.folders[i] %>')" style="cursor: pointer;">

                                        <img src="/images/glyphicons-208-remove.png" onclick="showModal('<%=session.folders[i] %>')" style="cursor: pointer;">

                                    </span>
                                    <a href='/folders/<%= session.folders[i] %>'><span><%= session.folders[i] %></span></a>
                                </li>
                        <% } %>
                        <li id="delete" hidden><span class="glyphicon glyphicon-trash"></span> REMOVE</li>
                        <br>
                    </ul>
                </li>
                <li>
                    <button id="nameFolder" onclick="nameFolder()" class="btn btn-success">Create Folder</button>
                </li> 
            </ul>
        </div>


        <div id="modal" class="modal fade" role="dialog">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title">Are you sure you want to delete this folder?</h4>
                    </div>
                    <div class="modal-body">
                            <button id="confirmDelete" type="button" class="btn btn-danger"></button>
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>